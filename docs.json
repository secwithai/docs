{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "SYNQ Bot",
  "description": "Complete command documentation for SYNQ Bot",
  "colors": {
    "primary": "#5865F2",
    "light": "#7289DA",
    "dark": "#4752C4"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Overview",
        "groups": [
          {
            "group": "Getting started",
            "pages": ["index", "premium"]
          }
        ]
      },
      {
        "tab": "Server Configuration",
        "groups": [
          {
            "group": "Setup",
            "pages": [
              "commands/invite-welcome",
              "commands/autorole",
              "commands/greet",
              "commands/jointocreate",
              "commands/customise"
            ]
          }
        ]
      },
      {
        "tab": "Tracking",
        "groups": [
          {
            "group": "Stats & leaderboards",
            "pages": [
              "commands/messages-voice",
              "commands/invites",
              "commands/leaderboard",
              "commands/add-remove-reset"
            ]
          }
        ]
      },
      {
        "tab": "Moderation",
        "groups": [
          {
            "group": "Moderation commands",
            "pages": ["commands/moderation"]
          }
        ]
      },
      {
        "tab": "Engagement",
        "groups": [
          {
            "group": "Giveaways & tools",
            "pages": [
              "commands/giveaway",
              "commands/boostrole",
              "commands/embed",
              "commands/reminder",
              "commands/timer"
            ]
          }
        ]
      },
      {
        "tab": "Automation",
        "groups": [
          {
            "group": "Automation",
            "pages": [
              "commands/autoresponder",
              "commands/customrole",
              "commands/guildtag",
              "commands/vanityrole",
              "commands/sticky",
              "commands/ticket",
              "commands/welcome"
            ]
          }
        ]
      },
      {
        "tab": "Utility & Info",
        "groups": [
          {
            "group": "Commands",
            "pages": ["commands/utility", "commands/information"]
          }
        ]
      },
      {
        "tab": "Developer",
        "groups": [
          {
            "group": "Owner commands",
            "pages": ["commands/developer"]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Support Server",
          "href": "https://discord.gg/synq",
          "icon": "discord"
        },
        {
          "anchor": "Invite Bot",
          "href": "https://discord.com/oauth2/authorize",
          "icon": "plus"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support Server",
        "href": "https://discord.gg/synq"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Invite Bot",
      "href": "https://discord.com/oauth2/authorize"
    }
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude", "perplexity"]
  },
  "footer": {
    "socials": {
      "discord": "https://discord.gg/synq",
      "github": "https://github.com"
    }
  }
}
